{
    "key_properties": ["id"],
    "schema": {
        "type": "object",
        "description": "The Employee model represents all Employee related information.\n Provides links to the Employee's associated Person, Legal Entity, Department, Earning, Deduction, Tax, Direct Deposit, Pay Rat and Custom Field information.",
        "additionalProperties": true,
        "required": [
            "id",
            "employeeNumber",
            "badgeNumber",
            "firstName",
            "middleName",
            "lastName",
            "email",
            "employmentDateData",
            "positionData",
            "statusData",
            "workLocation"
        ],
        "properties": {
            "id": {
                "type": "string",
                "description": "Unique identifier of the employee in Paycor's system. Generated by Paycor.",
                "format": "uuid"
            },
            "employeeNumber": {
                "type": [
                    "string",
                    "null"
                ],
                "description": "Unique number of the employee in the tenant. Generated by Paycor."
            },
            "badgeNumber": {
                "type": [
                    "string",
                    "null"
                ],
                "description": "Employee's assigned badge number - currently unavailable"
            },
            "firstName": {
                "type": [
                    "string",
                    "null"
                ],
                "description": "First name of the employee."
            },
            "middleName": {
                "type": [
                    "string",
                    "null"
                ],
                "description": "Middle name of the employee."
            },
            "lastName": {
                "type": [
                    "string",
                    "null"
                ],
                "description": "Last name of the employee."
            },
            "email": {
                "description": "Email Information of an employee.",
                "type": [
                    "object",
                    "null"
                ],
                "additionalProperties": false,
                "required": [
                    "type",
                    "emailAddress"
                ],
                "properties": {
                    "type": {
                        "type": "string",
                        "description": "Enumeration of valid email type values.",
                        "enum": [
                            "Home",
                            "Work"
                        ]
                    },
                    "emailAddress": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Email Address of the person."
                    }
                }
            },
            "employmentDateData": {
                "type": [
                    "object",
                    "null"
                ],
                "description": "The Employment Date Data represents the Employee's Employment Date information.",
                "additionalProperties": false,
                "required": [
                    "hireDate",
                    "adjustedHireDate",
                    "terminationDate",
                    "terminationReason",
                    "reHireDate",
                    "seniorityDate"
                ],
                "properties": {
                    "hireDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Date the employee was hired.\nFormat: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "adjustedHireDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Adjusted hire date is used when the employee's hire date does not accurately reflect benefits eligibility because an employee may have left, \nbeen rehired or taken a Leave of Absence. This provides an alternative date for the employee's accruals to be calculated on.\n Format: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "terminationDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Date the employee was terminated.\nFormat: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "terminationReason": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Reason for the employee's termination."
                    },
                    "reHireDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Re-hire date of the employee. Terminated employees can be rehired.\n Format: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "seniorityDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Date on which the calculation of an employee's seniority is based.\nFormat: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    }
                }
            },
            "positionData": {
                "type": [
                    "object",
                    "null"
                ],
                "description": "The Employee Position Data model represents the Employee's Position related data.",
                "additionalProperties": false,
                "required": [
                    "jobTitle",
                    "jobCode",
                    "effectiveStartDate",
                    "effectiveEndDate",
                    "payGroupId",
                    "manager"
                ],
                "properties": {
                    "jobTitle": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Name of the employee's job title."
                    },
                    "jobCode": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Code of the employee's job."
                    },
                    "effectiveStartDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Date the employee position data takes effect.\nFormat: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "effectiveEndDate": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Date the employee position data is no longer in effect.\nFormat: YYYY-MM-DDTHH:MM:SSZ  (ISO-8601 standard) "
                    },
                    "payGroupId": {
                        "type": "string",
                        "description": "Unique identifier of the pay group in Paycor's system. Generated by Paycor.",
                        "format": "uuid"
                    },
                    "manager": {
                        "type": [
                            "object",
                            "null"
                        ],
                        "description": "The Manager model represents information about the Employee's Manager.\nProvides link to the Manager's Employee level information.",
                        "additionalProperties": true,
                        "required": [
                            "id",
                            "employeeNumber"
                        ],
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "Unique identifier of the manager in Paycor's system. Generated by Paycor.",
                                "format": "uuid"
                            },
                            "employeeNumber": {
                                "type": [
                                    "string",
                                    "null"
                                ],
                                "description": "Employee number of the employee's manager."
                            }
                        }
                    }
                }
            },
            "statusData": {
                "type": [
                    "object",
                    "null"
                ],
                "description": "The Employee Status Data model represents Employee Employment Status information.",
                "additionalProperties": false,
                "required": [
                    "isFullTime",
                    "status",
                    "flsa",
                    "type"
                ],
                "properties": {
                    "isFullTime": {
                        "type": [
                            "boolean",
                            "null"
                        ],
                        "description": "Whether employee is full time. "
                    },
                    "status": {
                        "type": "string",
                        "description": "Enumeration of valid Employment Status values.",
                        "enum": [
                            "Unknown",
                            "Active",
                            "Deceased",
                            "LongTermDisability",
                            "ShortTermDisability",
                            "Fmla",
                            "LaidOff",
                            "LeaveWithPay",
                            "LeaveWithoutPay",
                            "ThirdPartyPayable",
                            "Resigned",
                            "Retired",
                            "Terminated",
                            "WorkersCompensation"
                        ]
                    },
                    "flsa": {
                        "type": "string",
                        "description": "Enumeration of valid Types of Fair Labor Standards Act (FLSA) employment values.",
                        "enum": [
                            "Unknown",
                            "HourlyExempt",
                            "HourlyNonExempt",
                            "SalaryExempt",
                            "SalaryNonExempt"
                        ]
                    },
                    "type": {
                        "type": "string",
                        "description": "Enumeration of valid Employment Type values.",
                        "enum": [
                            "Unknown",
                            "Casual",
                            "IndependentContractor",
                            "Regular",
                            "Seasonal",
                            "Temporary",
                            "Variable"
                        ]
                    }
                }
            },
            "workLocation": {
                "type": [
                    "object",
                    "null"
                ],
                "description": "The Employee Work Location Data model represents the Employee's Work Location related data.",
                "additionalProperties": false,
                "required": [
                    "streetLine1",
                    "type",
                    "city",
                    "county",
                    "state",
                    "zipCode",
                    "country"
                ],
                "properties": {
                    "streetLine1": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "First line of street address information."
                    },
                    "type": {
                        "type": "string",
                        "description": "Enumeration of valid Type of Work Location Address values.",
                        "enum": [
                            "Physical",
                            "Mailing",
                            "Unknown"
                        ]
                    },
                    "city": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "City in the address."
                    },
                    "county": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "County in the address."
                    },
                    "state": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "State in the address."
                    },
                    "zipCode": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Zip code in the address."
                    },
                    "country": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "description": "Country in the address."
                    }
                }
            }
        }
    }
}